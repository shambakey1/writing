\begin_layout Section
FBLT vs ECM
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)\le\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(2\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\label{eq:fblt_ecm_comp_1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\because\,|s_{i}|=\sum_{\forall s_{i}^{k}}$
\end_inset

, where 
\begin_inset Formula $|s_{i}|$
\end_inset

 is total number of transactions in any job of 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 Then (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_ecm_comp_1"

\end_inset

) becomes 
\begin_inset Formula 
\begin{equation}
m+|s_{i}|\left(\Omega_{i}^{k}+m-1\right)\le\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(2\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\label{eq:fblt_ecm_comp_2}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\Omega_{i}^{k}\le\frac{\left(\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(2\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\right)-\left(1+|s_{i}|\right)m+|s_{i}|}{|s_{i}|}\label{eq:fblt_ecm_comp_3}
\end{equation}

\end_inset


\begin_inset Formula $\because\,|s_{i}|\ge1$
\end_inset

, 
\begin_inset Formula $\therefore\,\frac{1+|s_{i}|}{|s_{i}|}\le2$
\end_inset

.
 Thus, (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_ecm_comp_3"

\end_inset

) holds if 
\begin_inset Formula 
\begin{equation}
\Omega_{i}^{k}\le2\left(\frac{\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)}{|s_{i}|}-m\right)\label{eq:fblt_ecm_comp_4}
\end{equation}

\end_inset


\begin_inset Formula $\because\,\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)$
\end_inset

 is the maximum number of transactions in all jobs with higher priority
 than current job of 
\begin_inset Formula $\tau_{i}$
\end_inset

 and have direct or indirect conflict with transactions in 
\begin_inset Formula $\tau_{i}$
\end_inset

, Claim follows.
\end_layout

\begin_layout Section
FBLT vs RCM
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)\le2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\label{eq:fblt_rcm_comp_1}
\end{equation}

\end_inset


\begin_inset Formula $\because\,|s_{i}|=\sum_{\forall s_{i}^{k}}$
\end_inset

, where 
\begin_inset Formula $|s_{i}|$
\end_inset

 is total number of transactions in any job of 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 
\begin_inset Formula $\because\,\Omega_{i}^{max}\ge\Omega_{i}^{k}$
\end_inset

, 
\begin_inset Formula $\therefore$
\end_inset

 (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_ecm_comp_1"

\end_inset

) becomes
\begin_inset Formula 
\begin{equation}
m+|s_{i}|\left(\Omega_{i}^{max}+m-1\right)\le2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\label{eq:fblt_rcm_comp_2}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\therefore\,\Omega_{i}^{max}\le\frac{\left(2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\right)-\left(1+|s_{i}|\right)m+|s_{i}|}{|s_{i}|}\label{eq:fblt_rcm_comp_3}
\end{equation}

\end_inset


\begin_inset Formula $\because\,|s_{i}|\ge1$
\end_inset

, 
\begin_inset Formula $\therefore\,\frac{1+|s_{i}|}{|s_{i}|}\le2$
\end_inset

.
 Thus, (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_rcm_comp_3"

\end_inset

) holds if 
\begin_inset Formula 
\begin{equation}
\Omega_{i}^{max}\le2\left(\frac{\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)}{|s_{i}|}-m\right)\label{eq:fblt_rcm_comp_4}
\end{equation}

\end_inset


\begin_inset Formula $\because\,\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)$
\end_inset

 is the maximum number of transactions in all jobs with higher priority
 than current job of 
\begin_inset Formula $\tau_{i}$
\end_inset

 and have direct or indirect conflict with transactions in 
\begin_inset Formula $\tau_{i}$
\end_inset

, Claim follows.
