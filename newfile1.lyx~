\begin_layout Standard
By taking the minimum lower bound and the maximum upper bound from the previous
 cases, Claim follows.
\end_layout

\begin_layout Section
G-RMA/LCM vs Global OMLP
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(1+\alpha_{max}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)s_{max}\right)\nonumber \\
+ & 2\left(1-\alpha_{min}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)s_{max}\right)\nonumber \\
+ & \left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)s_{max}\nonumber \\
\le & N_{i}\left(2m-1\right)L_{max}\label{eq:grma_lcm_vs_omlp_1}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\[
\frac{s_{max}}{L_{max}}\le\frac{N_{min}\left(2m-1\right)}{\left(n-1\right)\left(\left(1+\alpha_{max}\right)\left(\left(\Phi_{max}+1\right)N_{max}\right)+2\left(1-\alpha_{min}\right)N_{max}+\Phi_{max}\right)}
\]

\end_inset


\end_layout

\begin_layout Section
G-RMA/LCM vs RNLP
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(1+\alpha_{max}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)s_{max}\right)\nonumber \\
+ & 2\left(1-\alpha_{min}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)s_{max}\right)\nonumber \\
+ & \left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)s_{max}\nonumber \\
\le & N_{i}\left(2m-1\right)L_{max}\label{eq:grma_lcm_vs_omlp_1-1}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Section
G-EDF/PNF vs ECM
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(\left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor \right)s_{max}\nonumber \\
\le & \left(\left(\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(2\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\right)s_{max}\label{eq:pnf_vs_ecm_1}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor \nonumber \\
\le & \left(\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(2\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\label{eq:pnf_vs_ecm_2}
\end{eqnarray}

\end_inset

 
\begin_inset Formula $\because\,\gamma_{i}\subseteq\gamma_{i}^{ex}$
\end_inset

, 
\begin_inset Formula $\Theta_{i}\subseteq\Theta_{i}^{ex}$
\end_inset

 and 
\begin_inset Formula $2\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \ge\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1$
\end_inset

, 
\begin_inset Formula $\therefore\,\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\le\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(2\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)$
\end_inset

.
 So, (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:pnf_vs_ecm_2"

\end_inset

) holds if 
\begin_inset Formula $\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor \le\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor $
\end_inset

.
 
\begin_inset Formula $\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}$
\end_inset

 is the number of transactions in any task 
\begin_inset Formula $\tau_{j}\neq\tau_{i}$
\end_inset

 that has no direct conflict with any transaction in 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 
\begin_inset Formula $\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor $
\end_inset

 is the total number of higher priority jobs- that do not belong to 
\begin_inset Formula $\tau_{i}$
\end_inset

 - that can be released during 
\begin_inset Formula $T_{i}$
\end_inset

.
 Thus, Claim follows.
\end_layout

\begin_layout Section
G-RMA/PNF vs RCM
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(\left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}\right)}{m}\right\rfloor \right)s_{max}\nonumber \\
\le & \left(\left(2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\right)+\left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\right)s_{max}\label{eq:pnf_vs_rcm_1}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
 & \left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left\lfloor \frac{2\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}\right)}{m}\right\rfloor \nonumber \\
\le & \left(2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\label{eq:pnf_vs_rcm_2}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
\therefore & \left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left(2\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\right)\right)+\left\lfloor \frac{2\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}\right)}{m}\right\rfloor \nonumber \\
\le & \left(2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\label{eq:pnf_vs_rcm_3}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
Eq(
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:pnf_vs_rcm_3"

\end_inset

) holds if 
\begin_inset Formula 
\begin{eqnarray}
\therefore & \left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left(2\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\right)\right)+\left(2\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}\right)\right)\nonumber \\
\le & \left(2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\label{eq:pnf_vs_rcm_4}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
\therefore & \left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left(2\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l}}\right)\right)\nonumber \\
\le & \left(2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\label{eq:pnf_vs_rcm_5}
\end{eqnarray}

\end_inset

 
\begin_inset Formula $\because\,\gamma_{i}\subseteq\gamma_{i}^{ex}$
\end_inset

 and 
\begin_inset Formula $\Theta_{i}\subseteq\Theta_{i}^{ex}$
\end_inset

, 
\begin_inset Formula $\therefore\,\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)<2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)$
\end_inset

.
 Thus, (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:pnf_vs_rcm_5"

\end_inset

) holds if 
\begin_inset Formula $\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l}}\right)\le\frac{1}{2}\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil $
\end_inset

.
 
\begin_inset Formula $\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l}}\right)$
\end_inset

 is total number of transactions in tasks with lower priority than 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 
\begin_inset Formula $\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil $
\end_inset

 is total number of jobs with higher priority than 
\begin_inset Formula $p_{i}$
\end_inset

 that are released during 
\begin_inset Formula $T_{i}$
\end_inset

.
 Claim follows.
 
\end_layout

\begin_layout Section
G-EDF/PNF vs G-EDF/LCM
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(\left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor \right)s_{max}\nonumber \\
\le & \left(\left(\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\right)s_{max}\label{eq:pnf_vs_gedf_lcm_1}
\end{eqnarray}

\end_inset

 
\begin_inset Formula 
\begin{eqnarray}
 & \left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor \nonumber \\
\le & \left(\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\label{eq:pnf_vs_gedf_lcm_2}
\end{eqnarray}

\end_inset


\begin_inset Formula $\because\,\gamma_{i}\subseteq\gamma_{i}^{ex}$
\end_inset

 and 
\begin_inset Formula $\Theta_{i}\subseteq\Theta_{i}^{ex}$
\end_inset

.
 
\begin_inset Formula $\therefore$
\end_inset

 (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:pnf_vs_gedf_lcm_2"

\end_inset

) holds if 
\begin_inset Formula 
\begin{eqnarray}
 & \left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor \nonumber \\
\le & \left(\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\label{eq:pnf_vs_gedf_lcm_3}
\end{eqnarray}

\end_inset

Eq(
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:pnf_vs_gedf_lcm_3"

\end_inset

) holds if:
\end_layout

\begin_layout Enumerate
For each 
\begin_inset Formula $\tau_{i}$
\end_inset

 and 
\begin_inset Formula $\tau_{j}\in\gamma_{i}$
\end_inset

 
\begin_inset Formula 
\[
\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)\le\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)
\]

\end_inset


\begin_inset Formula 
\[
\therefore\,\forall\tau_{j}\in\gamma_{i},\,\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\le\left(1+\alpha_{max}\right)\left\lceil \frac{T_{i}}{T_{j}}\right\rceil 
\]

\end_inset


\begin_inset Formula 
\[
\therefore\,\forall\tau_{j}\in\gamma_{i},\,\frac{1}{\alpha_{max}}\le\left\lceil \frac{T_{i}}{T_{j}}\right\rceil 
\]

\end_inset

 By (
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
ref{eq:gedf_max_job_no_interfer_j_i}
\end_layout

\end_inset

), 
\begin_inset Formula $\left\lceil \frac{T_{i}}{T_{j}}\right\rceil $
\end_inset

 is maximum number of jobs of 
\begin_inset Formula $\tau_{j}$
\end_inset

 - with higher priority than current job of 
\begin_inset Formula $\tau_{i}$
\end_inset

 - that can exist during 
\begin_inset Formula $T_{i}$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor \le\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor $
\end_inset

.
 
\begin_inset Formula $\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}$
\end_inset

 is the number of transactions in any task 
\begin_inset Formula $\tau_{j}\neq\tau_{i}$
\end_inset

 that has no direct conflict with any transaction in 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 
\begin_inset Formula $\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor $
\end_inset

 is the total number of jobs- with higher priority than current job of 
\begin_inset Formula $\tau_{i}$
\end_inset

 - that can be released during 
\begin_inset Formula $T_{i}$
\end_inset

.
\end_layout

