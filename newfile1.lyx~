\end_inset

Effect of 
\begin_inset Formula $\lceil\frac{t(T_{i})}{t(T_{j})}\rceil$
\end_inset

 on 
\begin_inset Formula $\frac{s_{max}}{r_{max}}$
\end_inset


\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Section

\series bold
G-EDF/LCM vs ECM
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
 & \left(\left(\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\right)s_{max}\\
\le & \left(\left(\sum_{\forall\tau_{j}\in\gamma_{i}^{ex}}\left(2\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\right)s_{max}
\end{eqnarray*}

\end_inset

 
\begin_inset Formula 
\[
1+\alpha_{max}\le2
\]

\end_inset


\end_layout

\begin_layout Section
G-EDF/LCM vs Lock-free
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(\left(\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\right)s_{max}\nonumber \\
\le & \left(\left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)\right)r_{max}\label{eq:gedf_lcm_vs_lf_1}
\end{eqnarray}

\end_inset

Let 
\begin_inset Formula $\beta_{\{ij\}}^{*}=\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}$
\end_inset

, 
\begin_inset Formula $\therefore$
\end_inset

 (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:gedf_lcm_vs_lf_1"

\end_inset

) becomes 
\begin_inset Formula 
\[
\frac{s_{max}}{r_{max}}\le\frac{\left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)}{\left(\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \beta_{\{ij\}}^{*}\right)\right)+\left(\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)}
\]

\end_inset


\end_layout

\begin_layout Section
G-RMA/LCM vs.
 RCM
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
 & \left(1+\alpha_{max}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)s_{max}\right)\\
+ & \left(1-\alpha_{min}\right)\left(\sum_{\forall\tau_{z}\in\gamma_{i}^{ex},p_{z}<p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{z}}\right\rceil +1\right)\sum_{\forall s_{z}^{l},\left(\Theta=\Theta_{z}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)s_{max}\right)\\
+ & \left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)s_{max}\\
\le & \left(\left(2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\right)+\left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\right)s_{max}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\therefore & \left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\\
+ & \left(1-\alpha_{min}\right)\sum_{\forall\tau_{z}\in\gamma_{i}^{ex},p_{z}<p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{z}}\right\rceil +1\right)\sum_{\forall s_{z}^{l},\left(\Theta=\Theta_{z}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\\
\le & 2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\therefore & \left(1-\alpha_{min}\right)\sum_{\forall\tau_{z}\in\gamma_{i}^{ex},p_{z}<p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{z}}\right\rceil +1\right)\sum_{\forall s_{z}^{l},\left(\Theta=\Theta_{z}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)\\
\le & \left(1-\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\therefore & \frac{1-\alpha_{min}}{1-\alpha_{max}}\\
\le & \frac{\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)}{\sum_{\forall\tau_{z}\in\gamma_{i}^{ex},p_{z}<p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{z}}\right\rceil +1\right)\sum_{\forall s_{z}^{l},\left(\Theta=\Theta_{z}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\because p_{z}<p_{i},\,\therefore T_{i}<T_{z}$
\end_inset

 by definition of G-RMA and implicit deadline tasks.
 
\begin_inset Formula $\therefore\left\lceil \frac{T_{i}}{T_{z}}\right\rceil =1,\,\forall\tau_{i},\tau_{z}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\therefore & \frac{1-\alpha_{min}}{1-\alpha_{max}}\\
\le & \frac{\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)}{2\sum_{\forall\tau_{z}\in\gamma_{i}^{ex},p_{z}<p_{i}}\sum_{\forall s_{z}^{l},\left(\Theta=\Theta_{z}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Section
G-RMA/LCM vs Lock-free
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(1+\alpha_{max}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)s_{max}\right)\nonumber \\
+ & \left(1-\alpha_{min}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}<p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)s_{max}\right)\nonumber \\
+ & \left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)s_{max}\nonumber \\
\le & \left(\left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}\right)+\left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\right)r_{max}\label{eq:grma_lcm_vs_lf_3}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
By definition of G-RMA and implicit deadline tasks, 
\begin_inset Formula $\left\lceil \frac{T_{i}}{T_{j}}\right\rceil =1,\,\forall\tau_{i},\,\tau_{j}$
\end_inset

 where 
\begin_inset Formula $p_{j}<p_{i}$
\end_inset

.
 So, (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:grma_lcm_vs_lf_3"

\end_inset

) becomes 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(1+\alpha_{max}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)s_{max}\right)\nonumber \\
+ & 2\left(1-\alpha_{min}\right)\left(\sum_{\forall\tau_{j}\in\gamma_{i},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}\right)\neq\emptyset}\right)s_{max}\right)\nonumber \\
+ & \left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)s_{max}\nonumber \\
\le & \left(\left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}\right)+\left(\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)\right)r_{max}\label{eq:grma_lcm_vs_lf_4}
\end{eqnarray}

\end_inset

 The set of tasks 
\begin_inset Formula $\left\{ \tau_{j}|\tau_{j}\neq\tau_{i}\right\} $
\end_inset

 can be devided into four sets dependinong on priority and object sharing
 between 
\begin_inset Formula $\tau_{i}$
\end_inset

 and 
\begin_inset Formula $\tau_{j}$
\end_inset

.
 So, 
\begin_inset Formula $\left\{ \tau_{j}|\tau_{j}\neq\tau_{i}\right\} =\left\{ \tau_{l}\right\} \cup\left\{ \tilde{\tau}_{l}\right\} \cup\left\{ \tau_{h}\right\} \cup\left\{ \tilde{\tau}_{h}\right\} $
\end_inset

, where:
\end_layout

