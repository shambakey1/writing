\begin_layout Section
FBLT vs G-RMA/LCM
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)\label{eq:fblt_grma_lcm_comp_1}\\
\le & \left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\nonumber \\
+ & \left(1-\alpha_{min}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}<p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\nonumber 
\end{eqnarray}

\end_inset


\begin_inset Formula $\because\,|s_{i}|=\sum_{\forall s_{i}^{k}}$
\end_inset

, where 
\begin_inset Formula $|s_{i}|$
\end_inset

 is total number of transactions in any job of 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 Then (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_grma_lcm_comp_1"

\end_inset

) becomes
\begin_inset Formula 
\begin{eqnarray}
 & m+|s_{i}|\left(\Omega_{i}^{k}+m-1\right)\label{eq:fblt_grma_lcm_comp_2}\\
\le & \left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\nonumber \\
+ & 2\left(1-\alpha_{min}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\nonumber 
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
 & \therefore\,\Omega_{i}^{k}\label{eq:fblt_grma_lcm_comp_3}\\
\le & \frac{\left(\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\right)}{|s_{i}|}\nonumber \\
+ & \frac{\left(2\left(1-\alpha_{min}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\right)}{|s_{i}|}\nonumber \\
+ & 1-\frac{1+|s_{i}|}{|s_{i}|}m\nonumber 
\end{eqnarray}

\end_inset


\begin_inset Formula $\because\,|s_{i}|\ge1$
\end_inset

, 
\begin_inset Formula $\therefore\,\frac{1+|s_{i}|}{|s_{i}|}\le2$
\end_inset

.
 Thus, (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_grma_lcm_comp_3"

\end_inset

) holds if 
\begin_inset Formula 
\begin{eqnarray}
 & \Omega_{i}^{k}\label{eq:fblt_grma_lcm_comp_4}\\
\le & \frac{\left(\left(1+\alpha_{max}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\right)}{|s_{i}|}\nonumber \\
+ & \frac{\left(2\left(1-\alpha_{min}\right)\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)\right)}{|s_{i}|}-2m\nonumber 
\end{eqnarray}

\end_inset


\begin_inset Formula $\because\,\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}>p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}^{ex}\neq\emptyset}\right)$
\end_inset

 is the maximum number of transactions in all jobs with higher priority
 than current job of 
\begin_inset Formula $\tau_{i}$
\end_inset

 and have direct or indirect conflict with transactions in 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 
\begin_inset Formula $\because\,2\sum_{\forall\tau_{j}\in\gamma_{i}^{ex},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{l},\left(\Theta=\Theta_{j}^{l}\cap\Theta_{i}^{ex}\right)\neq\emptyset}\right)$
\end_inset

 is the maximum number of transactions in all jobs with lower priority than
 current job of 
\begin_inset Formula $\tau_{i}$
\end_inset

 and have direct or indirect conflict with transactions in 
\begin_inset Formula $\tau_{i}$
\end_inset

, Claim follows.
\end_layout

\begin_layout Section
FBTL vs.
 G-EDF/PNF
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \nonumber \\
\le & \sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor \label{eq:fblt_vs_gedf_pnf_1}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
\therefore & |s_{i}|\Omega_{i}^{k}\nonumber \\
\le & \sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor -\left(1+|s_{i}|\right)m+|s_{i}|-\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \label{eq:fblt_vs_gedf_pnf_2}
\end{eqnarray}

\end_inset


\begin_inset Formula $\because\,|s_{i}|\ge1$
\end_inset

, 
\begin_inset Formula $\therefore\,\frac{1+|s_{i}|}{|s_{i}|}\le2$
\end_inset

 and
\begin_inset Formula $\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \ge\frac{\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor }{|s_{i}|}$
\end_inset

.
 Thus, (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_vs_gedf_pnf_2"

\end_inset

) holds if
\begin_inset Formula 
\begin{eqnarray}
\therefore & \Omega_{i}^{k}\nonumber \\
\le & \frac{\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j}}\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}}{m}\right\rfloor }{|s_{i}|}-\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor -2m\label{eq:fblt_vs_gedf_pnf_3}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Section
FBLT vs G-RMA/PNF
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)+\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \nonumber \\
\le & \sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)+\left\lfloor \frac{\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}\right)}{m}\right\rfloor \label{eq:fblt_vs_grma_pnf_1}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
 & |s_{i}|\Omega_{i}^{k}\nonumber \\
\le & \sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)+\left\lfloor \frac{2\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}\right)}{m}\right\rfloor -\left(1+|s_{i}|\right)m+|s_{i}|-\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \label{eq:fblt_vs_grma_pnf_2}
\end{eqnarray}

\end_inset


\begin_inset Formula $\because\,|s_{i}|\ge1$
\end_inset

, 
\begin_inset Formula $\therefore\,\frac{1+|s_{i}|}{|s_{i}|}\le2$
\end_inset

 and
\begin_inset Formula $\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \ge\frac{\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil }{|s_{i}|}$
\end_inset

.
 Thus, (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_vs_grma_pnf_2"

\end_inset

) holds if
\begin_inset Formula 
\begin{eqnarray}
 & \Omega_{i}^{k}\nonumber \\
\le & \frac{\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\sum_{\forall s_{j}^{l},\Theta_{j}^{l}\cap\Theta_{i}\neq\emptyset}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\right)+\left\lfloor \frac{2\sum_{\forall\tau_{j},p_{j}<p_{i}}\left(\sum_{\forall s_{j}^{h},\Theta_{j}^{h}\cap\Theta_{i}=\emptyset}\right)}{m}\right\rfloor }{|s_{i}|}-2m-\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \label{eq:fblt_vs_grma_pnf_3}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Section
FBLT vs G-EDF/Lock-free
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)s_{max}\nonumber \\
\le & \left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)r_{max}\label{eq:fblt_vs_gedf_lf_1}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{equation}
\frac{s_{max}}{r_{max}}\le\frac{\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor }{m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor }\label{eq:fblt_vs_gedf_lf_2}
\end{equation}

\end_inset


\begin_inset Formula $\because\,|s_{i}|=\sum_{\forall s_{i}^{k}}$
\end_inset

, where 
\begin_inset Formula $|s_{i}|$
\end_inset

 is total number of transactions in any job of 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 
\begin_inset Formula $\because\,\Omega_{i}^{max}\ge\Omega_{i}^{k}$
\end_inset

, 
\begin_inset Formula $\therefore$
\end_inset

 (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_vs_gedf_lf_2"

\end_inset

) holds if 
\begin_inset Formula 
\[
\frac{s_{max}}{r_{max}}\le\frac{\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor }{m+|s_{i}|\left(\Omega_{i}^{max}+m-1\right)+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor }
\]

\end_inset


\end_layout

\begin_layout Section
FBLT vs G-RMA/Lock-free
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)+\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)s_{max}\nonumber \\
\le & \left(\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}+\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil \right)r_{max}\label{eq:fblt_vs_grma_lf_1}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\,\frac{s_{max}}{r_{max}}\le\frac{\sum_{\forall\tau_{j}\in\gamma_{i}}\left(\left\lceil \frac{T_{i}}{T_{j}}\right\rceil +1\right)\beta_{ij}+\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil }{m+|s_{i}|\left(\Omega_{i}^{max}+m-1\right)+\sum_{\forall\tau_{j},p_{j}>p_{i}}\left\lceil \frac{T_{i}}{T_{j}}\right\rceil }
\]

\end_inset


\end_layout

\begin_layout Section
FBLT vs G-EDF/OMLP
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
 & \left(m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor \right)s_{max}\nonumber \\
\le & N_{i}\left(2m-1\right)L_{max}\label{eq:fblt_vs_gedf_omlp_1}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{equation}
\frac{s_{max}}{L_{max}}\le\frac{N_{i}\left(2m-1\right)}{m+\sum_{\forall s_{i}^{k}}\left(\Omega_{i}^{k}+m-1\right)+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor }\label{eq:fblt_vs_gedf_omlp_2}
\end{equation}

\end_inset


\begin_inset Formula $\because\,|s_{i}|=\sum_{\forall s_{i}^{k}}$
\end_inset

, where 
\begin_inset Formula $|s_{i}|$
\end_inset

 is total number of transactions in any job of 
\begin_inset Formula $\tau_{i}$
\end_inset

.
 
\begin_inset Formula $\because\,\Omega_{i}^{max}\ge\Omega_{i}^{k}$
\end_inset

, 
\begin_inset Formula $\therefore$
\end_inset

 (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fblt_vs_gedf_omlp_2"

\end_inset

) holds if for each 
\begin_inset Formula $\tau_{i}$
\end_inset

 
\begin_inset Formula 
\begin{equation}
\frac{s_{max}}{L_{max}}\le\frac{N_{i}\left(2m-1\right)}{m+|s_{i}|\left(\Omega_{i}^{max}+m-1\right)+\sum_{\forall\tau_{j}\in\zeta_{i}}\left\lfloor \frac{T_{i}}{T_{j}}\right\rfloor }\label{eq:fblt_vs_gedf_omlp_3}
\end{equation}

\end_inset


\end_layout

